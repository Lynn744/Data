package jdc_revision;

public class PassByReference {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int[] values = {10, 11, 12};
		System.out.print("Before: ");
		for(int a: values) {
			System.out.println(a+" ");
		}
		increase(values);
		
		System.out.print("Before: ");
		for(int a: values) {
			System.out.println(a+" ");
		}
	}
	
	 static void increase(int[] a) {
		for(int value:a) {
			value+=5;
	 	}
	}
}