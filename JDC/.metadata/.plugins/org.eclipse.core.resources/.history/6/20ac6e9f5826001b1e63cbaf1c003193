package jdc_revision;

public class Even_odd {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		EvenOdd(10,5,2,4);
	}
	static void EvenOdd(int num1, int num2, int num3, int num4) {
		if(num1%2==0) {
			System.out.printf("%d is an even number%n",num1,num2,num3,num4);
		}
	}

}
